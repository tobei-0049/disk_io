###
MAX_PROC   8
PROTOKOLL_DIR  .


WRITE_FILE N_PROC 1 SIZE=1m  BS=1024 PREFIX=b_f     PROTOKOLL_FILE=wr_block.prot
WRITE_FILE N_PROC 1 SIZE=1m  BS=16k  PREFIX=b_f_16k PROTOKOLL_FILE=wr_block.prot
WRITE_FILE N_PROC 1 SIZE=1m  BS=32k  PREFIX=b_f_32k PROTOKOLL_FILE=wr_block.prot
WRITE_FILE N_PROC 1 SIZE=1m  BS=4m   PREFIX=b_f_b1  PROTOKOLL_FILE=wr_block.prot
WRITE_FILE N_PROC 1 SIZE=10m BS=4m   PREFIX=b_f_b2  PROTOKOLL_FILE=wr_block.prot


READ_FILE  N_PROC 1  BS=1024 PREFIX=b_f       PROTOKOLL_FILE=rd_block.prot
READ_FILE  N_PROC 1  BS=16k  PREFIX=b_f_16k   PROTOKOLL_FILE=rd_block.prot
READ_FILE  N_PROC 1  BS=32k  PREFIX=b_f_32k   PROTOKOLL_FILE=rd_block.prot
READ_FILE  N_PROC 1  BS=4m   PREFIX=b_f_b1    PROTOKOLL_FILE=rd_block.prot
READ_FILE  N_PROC 1  BS=4m   PREFIX=b_f_b2    PROTOKOLL_FILE=rd_block.prot

REM_FILES  1   N_PROC 1 PREFIX=b_f     PROTOKOLL_FILE=/dev/null
REM_FILES  1   N_PROC 1 PREFIX=b_f_16k PROTOKOLL_FILE=/dev/null
REM_FILES  1   N_PROC 1 PREFIX=b_f_32k PROTOKOLL_FILE=/dev/null
REM_FILES  1   N_PROC 1 PREFIX=b_f_b1  PROTOKOLL_FILE=/dev/null
REM_FILES  1   N_PROC 1 PREFIX=b_f_b2  PROTOKOLL_FILE=/dev/null


WRITE_FILE  N_PROC 8  SIZE=5m  BS=1024 PREFIX=b_f PROTOKOLL_FILE=wr_block.prot
WRITE_FILE  N_PROC 8  SIZE=5m  BS=1m PREFIX=b_f_b PROTOKOLL_FILE=wr_block.prot

READ_FILE   N_PROC 8  BS=1k   PREFIX=b_f   PROTOKOLL_FILE=rd_block.prot
READ_FILE   N_PROC 8  BS=4m   PREFIX=b_f_b PROTOKOLL_FILE=/dev/null

REM_FILES  1   N_PROC 8   PREFIX=b_f    PROTOKOLL_FILE=/dev/null
REM_FILES  1   N_PROC 8   PREFIX=b_f_b  PROTOKOLL_FILE=/dev/null

